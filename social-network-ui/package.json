{
	"name": "social-network-ui",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"npm-run-all": "^4.1.1",
		"npm-watch": "^0.3.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.2",
		"react-router-dom": "^6.15.0",
		"react-scripts": "5.0.1",
		"stylelint": "^8.2.0",
		"stylelint-config-standard": "^17.0.0",
		"stylelint-order": "^0.7.0",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-styles-sass start-js",
		"build": "npm run stylelint-build && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
		"stylelint-build": "stylelint 'src/**/*.scss'",
		"watch-styles-sass": "npm-watch",
		"stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'"
	},
	"watch": {
		"stylelint-dev": {
			"patterns": [
				"/src/**/*.scss"
			],
			"extensions": "scss"
		}
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"babel-eslint": "^10.1.0",
		"standard": "^10.0.3",
		"@babel/eslint-parser": "^7.21.3"
	}
}
